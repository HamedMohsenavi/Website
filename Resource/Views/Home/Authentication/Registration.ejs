<%- include(ViewPath('Errors')) %>

<form action="/Authentication/Registration" method="POST">
    <input type="text" name="Name" id="Name" placeholder="Name" value="<%= GetFormData('Name') %>"> <br>
    <input type="email" name="Email" id="Email" placeholder="Email" value="<%= GetFormData('Email') %>"> <br>
    <input type="password" name="Password" id="Password" placeholder="Password">
    <%- Recaptcha %>
    <button type="submit">Register</button>
    <a href="/Authentication/Google">Google</a>
</form>
